[precondition]
name=4k
ioengine=libaio
direct=1
bs=1m
rw=write
iodepth=4
numjobs=32
buffered=0
size=100%
loops=2
randrepeat=0
norandommap
refill_buffers
write_bw_log=${OUTPUT}/${MODE}-iodepth-4-numjobs-32
write_lat_log=${OUTPUT}/${MODE}-iodepth-4-numjobs-32
write_iops_log=${OUTPUT}/${MODE}-iodepth-4-numjobs-32
log_avg_msec=${LOGINTERVAL}